        xmlns:local="clr-namespace:Selection.Views"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TabControl>
            <TabItem Header="treemap">
                <local:TreeMapSelection />
            </TabItem>
            <TabItem Header="pivotmap">
                <local:PivotMapSelection />
            </TabItem>
        </TabControl>
    </Grid>
</Window>
